generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model Historico {
  id               String       @id @default(uuid())
  item_id          Int 
  doador_id        Int  
  receptor_id      Int 
  data_transacao   DateTime
  tipo             String
}
